{
	"manifest_version": 2,
	"name": "Page2Qrcode",
	"version": "1.0.0",
	"description": "Page2Qrcode 将当前页面标签地址快速生成二维码",
	"icons":
	{
		"16": "img/i16.png",
		"48": "img/i48.png",
		"128": "img/i128.png"
	},
	"browser_action": 
	{
		"default_icon": "img/i128.png",
		"default_title": "Page2Qrcode",
		"default_popup": "popup.html"
	},
	"permissions":
	[
		"declarativeContent",
		"tabs"
	]
}